{
  "name": "my-portfolio",
  "type": "module",
  "version": "0.0.1",
  "description": "A modern, interactive 3D portfolio website showcasing expertise in data analytics and HR analytics",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "node dev.mjs",
    "start": "astro preview",
    "build": "astro build",
    "preview": "astro preview",
    "clean": "rimraf .astro .vite dist node_modules/.vite",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "validate": "npm run lint && npm run type-check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.0",
    "@astrojs/tailwind": "^6.0.0",
    "@astrojs/vercel": "^8.0.8",
    "@headlessui/react": "^1.7.17",
    "@motionone/dom": "^10.17.0",
    "@phosphor-icons/react": "^2.0.15",
    "@supabase/supabase-js": "^2.39.3",
    "astro": "^5.3.1",
    "axios": "^1.6.7",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "framer-motion": "^10.16.4",
    "jsonwebtoken": "^9.0.2",
    "kill-port-process": "^3.2.1",
    "lottie-web": "^5.12.2",
    "openai": "^4.26.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "tailwindcss": "^3.4.1",
    "three": "^0.161.0",
    "validator": "^13.11.0",
    "vite": "^6.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/axios": "^0.14.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/three": "^0.161.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.31.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-astro": "^0.13.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml}": [
      "prettier --write"
    ]
  }
}
